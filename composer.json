{
    "name": "tm/easylightbox",
    "type": "magento-module",
    "homepage": "https://github.com/tmhub/easylightbox",
    "license": "OSL-3.0",
    "description": "Lightbox library integration",
    "version": "3.0.0",
    "authors": [
        {
            "name": "Templates-Master Team",
            "email": "support@templates-master.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "tm/core": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://tmhub.github.io/packages/"
        }
    ],
    "extra": {
        "map": [
            ["app/code/community/TM/EasyLightbox","app/code/community/TM/EasyLightbox"],
            ["app/design/adminhtml/default/default/layout/tm/easylightbox.xml","app/design/adminhtml/default/default/layout/tm/easylightbox.xml"],
            ["app/design/frontend/base/default/layout/tm/easylightbox.xml","app/design/frontend/base/default/layout/tm/easylightbox.xml"],
            ["app/design/frontend/base/default/template/tm/easylightbox","app/design/frontend/base/default/template/tm/easylightbox"],
            ["app/etc/modules/TM_EasyLightbox.xml","app/etc/modules/TM_EasyLightbox.xml"],
            ["app/locale/en_US/TM_EasyLightbox.csv","app/locale/en_US/TM_EasyLightbox.csv"],
            ["js/tm/easylightbox","js/tm/easylightbox"],
            ["media/easylightbox","media/easylightbox"]
        ],
        "magento-root-dir":".",
        "magento-deploystrategy": "copy",
        "magento-force": true,
        "changelog": ""
    }
}
